git init  'شروع کار با گیت'
git status ' رو مشخص می کنه git  وضعیت دایکتوری'
git add -A OR 'namefile' 'اضافه کردن فایل به استیج'
git checkout --namefile   'کل تغییرات فایل رو کنسل میکنه'
git reset namefile  'از استیج خارج می کنه'
git add  'name*'  '  اضافه کردن فایل به استیج باشروع نام فایل مشخص'
git commit -m 'mes' 'فایل کامیت میکنه با کامنت'
git log  'لاگ کارهای انجام شده مشخص می شود'
git diff head  'تغییرا رو با آخرین ورؤن چک میکنه'
git diff staged   'تغییرات فایلهای داخل استیج رو با آخرین وضعیت چک می کنه'
git branch 'شاخه های ایجاد شده رو نمایش میده'
git branch namebranch 'شاخه جدید ایجاد میکند'
git checkout namebranch  'سویچ تو شاخه'
git merge namebranch   'یکی کردن با شاخه جدید'